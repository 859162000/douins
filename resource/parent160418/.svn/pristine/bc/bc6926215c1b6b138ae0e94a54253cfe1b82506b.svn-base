package com.douins.account.domain.vo;

import java.io.Serializable;

public class UserPayAccountVo implements Serializable {
	private static final long serialVersionUID = 1L;
	private String accountId;// 支付账号ID
	private String userAccountId;// 虚拟账户ID
	private String bankCode;
	private String accountType;// 账号类型 1银行卡，2微信，3支付宝
	private String accountName;// 账户名称（银行名称）、支付宝、微信
	private String accountNo;// 账户号（银行卡号）、支付宝账号、微信账号
	private String cardholderName;//姓名
	private String dailyLimit;
	private String everyLimit;

	public String getBankCode() {
		return bankCode;
	}
	public void setBankCode(String bankCode) {
		this.bankCode = bankCode;
	}
	public String getDailyLimit() {
		return dailyLimit;
	}
	public void setDailyLimit(String dailyLimit) {
		this.dailyLimit = dailyLimit;
	}
	public String getEveryLimit() {
		return everyLimit;
	}
	public void setEveryLimit(String everyLimit) {
		this.everyLimit = everyLimit;
	}
	public String getAccountId() {
		return accountId;
	}
	public void setAccountId(String accountId) {
		this.accountId = accountId;
	}
	public String getAccountType() {
		return accountType;
	}
	public void setAccountType(String accountType) {
		this.accountType = accountType;
	}
	public String getAccountName() {
		return accountName;
	}
	public void setAccountName(String accountName) {
		this.accountName = accountName;
	}
	public String getAccountNo() {
		return accountNo;
	}
	public void setAccountNo(String accountNo) {
		this.accountNo = accountNo;
	}
	public String getUserAccountId() {
		return userAccountId;
	}
	public void setUserAccountId(String userAccountId) {
		this.userAccountId = userAccountId;
	}
	public String getCardholderName() {
		return cardholderName;
	}
	public void setCardholderName(String cardholderName) {
		this.cardholderName = cardholderName;
	}

}
