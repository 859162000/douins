package com.douins.agency.service.douins.domain.vo;

/** 
* @ClassName: ProductVo 
* @Description: 产品信息封装对象
* @author G. F. 
* @date 2016年1月1日 上午11:17:20 
*  
*/
public class ProductVo {
    private String productId;           // 豆芽内部产品id
    private String insurePdId;    //保险公司的产品id
    private String clauseCode;    //条款代码
    private String kindCode;      //责任代码
    private String amount;        //保额
    private String premium;     // 条款对应的保费
    private String saleStrategy;    // 销售策略枚举值
    
    public String getProductId() {
        return productId;
    }
    public void setProductId(String productId) {
        this.productId = productId;
    }
    public String getInsurePdId() {
        return insurePdId;
    }
    public void setInsurePdId(String insurePdId) {
        this.insurePdId = insurePdId;
    }
    public String getClauseCode() {
        return clauseCode;
    }
    public void setClauseCode(String clauseCode) {
        this.clauseCode = clauseCode;
    }
    public String getKindCode() {
        return kindCode;
    }
    public void setKindCode(String kindCode) {
        this.kindCode = kindCode;
    }
    public String getAmount() {
        return amount;
    }
    public void setAmount(String amount) {
        this.amount = amount;
    }
    public String getPremium() {
        return premium;
    }
    public void setPremium(String premium) {
        this.premium = premium;
    }
    public String getSaleStrategy() {
        return saleStrategy;
    }
    public void setSaleStrategy(String saleStrategy) {
        this.saleStrategy = saleStrategy;
    }
}
