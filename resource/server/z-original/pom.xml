	<build>
		<finalName>fortune</finalName>
		<plugins>
			<plugin>
				<groupId>org.mortbay.jetty</groupId>
				<artifactId>maven-jetty-plugin</artifactId>
				<configuration>
					<scanTargets>
						<scanTarget>src/main/java</scanTarget>
						<scanTarget>src/main/webapp/WEB-INF</scanTarget>
						<scanTarget>src/main/resources</scanTarget>
					</scanTargets>
					<scanTargetPatterns>
						<scanTargetPattern>
							<directory>src/main</directory>
							<includes>
								<include>**/*.java</include>
								<include>**/*.properties</include>
								<include>**/*.xml</include>
							</includes>
							<excludes>
								<exclude>**/*.js</exclude>
								<exclude>**/*.css</exclude>
								<exclude>**/*.jsp</exclude>
							</excludes>
						</scanTargetPattern>
					</scanTargetPatterns>
					<connectors>
						<connector implementation="org.mortbay.jetty.nio.SelectChannelConnector">
							<port>8081</port>
							<maxIdleTime>60000</maxIdleTime>
						</connector>
					</connectors>
					<contextPath>/</contextPath>
					<webDefaultXml>src/main/resources/jetty/webdefault.xml</webDefaultXml>
					<scanIntervalSeconds>100000</scanIntervalSeconds>
				</configuration>
			</plugin>
		</plugins>
	</build>

